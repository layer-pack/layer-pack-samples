{
  "name": "childProject",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "main": "./Server.js",
  "author": "Nathan Braun <caipilabs@gmail.com>",
  "wpInherit": {
    "default": {
      "rootFolder": "App",
      "extend": [
        "parentProject",
        "webpack4-with-es6-and-sass"
      ]
    },
    "api": {
      "rootFolder": "App",
      "extend": [
        "parentProject",
        "webpack4-with-es6-and-sass"
      ]
    }
  },
  "scripts": {
    "build": "wpi& wpi :api",
    "dev-api": "wpi :api -w",
    "start-dev": "concurrently \"wpi-dev-server --hot\" \"wpi :api -w\" \"sleep 1&&nodemon --watch App --inspect=[::]:9229 ./Server.js -p 9501\"",
    "start": "node ./Server.js -p 9001",
    "runBrowser": "start http://localhost:9001"
  },
  "dependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7",
    "parentProject": "file:../parentProject"
  },
  "devDependencies": {
    "webpack-inherit": "^1.0.9",
    "webpack4-with-es6-and-sass": "file:../webpack4-with-es6-and-sass"
  }
}
