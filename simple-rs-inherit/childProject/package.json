{
  "name": "childProject",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "main": "./Server.js",
  "author": "Nathan Braun <caipilabs@gmail.com>",
  "wpInherit": {
    "default": {
      "rootFolder": "App",
      "extend": [
        "parentProject",
        "wp4-react-rescope-sass-hmr"
      ]
    },
    "api": {
      "rootFolder": "App",
      "extend": [
        "parentProject",
        "wp4-react-rescope-sass-hmr"
      ]
    }
  },
  "scripts": {
    "build": "wpi&&wpi :api -w",
    "dev-api": "wpi :api -w",
    "start-dev": "concurrently \"wpi-dev-server --hot\" \"wpi :api -w\" \"sleep 3&&nodemon --watch ./dist/App.server.js --inspect=[::]:9229 ./Server.js -p 9701\"",
    "start": "node ./Server.js -p 9501",
    "runBrowser": "start http://localhost:9501"
  },
  "dependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7",
    "parentProject": "file:../parentProject"
  },
  "devDependencies": {
    "webpack-inherit": "^1.2.2",
    "wp4-react-rescope-sass-hmr": "file:../wp4-react-rescope-sass-hmr"
  }
}
