{
  "name": "childProject",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "main": "./Server.js",
  "author": "Nathan Braun <n8tz.js@gmail.com>",
  "wpInherit": {
    "default": {
      "rootFolder": "App",
      "vars": {},
      "extend": [
        "parentProject",
        "wp4-react-rescope-sass-hmr"
      ]
    },
    "devClient": {
      "rootFolder": "App",
      "basedOn": "default",
      "extend": [
        "parentProject",
        "wp4-react-rescope-sass-hmr"
      ]
    },
    "api": {
      "rootFolder": "App",
      "vars": {
        "externals": true
      },
      "extend": [
        "parentProject",
        "wp4-react-rescope-sass-hmr"
      ]
    }
  },
  "scripts": {
    "build": "wpi&&wpi :api",
    "dev-api": "wpi :api -w",
    "start-dev": "concurrently \"wpi-dev-server :devClient --hot\" \"wpi :api -w\" \"sleep 3&&nodemon --watch ./dist/App.server.js --inspect=[::]:9229 ./dist/App.server.js  -p 9701\"",
    "start": "node ./dist/App.server.js  -p 9501",
    "runBrowser": "start http://localhost:9501"
  },
  "dependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "devDependencies": {
    "webpack-inherit": "^1.4.2",
    "wp4-react-rescope-sass-hmr": "file:../wp4-react-rescope-sass-hmr",
    "parentProject": "file:../parentProject"
  }
}
