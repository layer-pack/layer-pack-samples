{
  "default": {
    "rootFolder": "App"
  },
  "api": {
    "rootFolder": "App",
    "config": "./webpack.config.api.js",
    "vars": {
      "targetDir": "dist/api",
      "externals": true,
      "DefinePluginCfg": {
        "__IS_SERVER__": true
      }
    }
  },
  "wwwProd": {
    "rootFolder": "App",
    "config": "./webpack.config.www.prod.js",
    "vars": {
      "targetDir": "dist/www",
      "production": true,
      "babelPreset": {
        "targets": {
          "browsers": "> 0.25%, not dead, safari >= 6, ios >= 6, chrome >= 52"
        },
        "loose": true
      }
    }
  },
  "www": {
    "rootFolder": "App",
    "config": "./webpack.config.www.dev.js",
    "vars": {
      "targetDir": "dist/www",
      "babelPreset": {
        "targets": {
          "browsers": "> 0.25%, not dead, safari >= 6, ios >= 6, chrome >= 52"
        },
        "loose": true
      }
    }
  }
}