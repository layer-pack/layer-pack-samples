{
  "name": "simple-redux-app",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "main": "./Server.js",
  "author": "Nathan Braun <n8tz.js@gmail.com>",
  "wpInherit": {
    "default": {
      "rootFolder": "App",
      "libsPath": "./libs",
      "vars": {
        "production": true
      },
      "extend": [
        "wpi-react-redux-sass-ssr"
      ]
    },
    "devClient": {
      "rootFolder": "App",
      "basedOn": "default",
      "libsPath": "./libs",
      "vars": {
        "webpackPatch": {
          "watchOptions": {
            "aggregateTimeout": 300,
            "poll": 1000
          }
        }
      },
      "extend": [
        "wpi-react-redux-sass-ssr"
      ]
    },
    "api": {
      "rootFolder": "App",
      "libsPath": "./libs",
      "vars": {
        "externals": true,
        "webpackPatch": {
          "devtool": "eval",
          "watchOptions": {
            "aggregateTimeout": 300,
            "poll": 1000
          }
        }
      },
      "extend": [
        "wpi-react-redux-sass-ssr"
      ]
    }
  },
  "scripts": {
    "build": "wpi&&wpi :api",
    "dev-api": "wpi :api -w",
    "start-dev": "wpi :api&&concurrently \"wpi-dev-server :devClient --hot --host 0.0.0.0\" \"wpi :api -w\" \"sleep 3&&nodemon --watch ./dist/App.server.js --inspect=[::]:9229 ./dist/App.server.js  -p 9701\"",
    "start": "node ./dist/App.server.js  -p 8080",
    "runBrowser": "start http://localhost:8080"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "body-parser": "^1.18.2",
    "concurrently": "^4.1.0",
    "debounce": "^1.2.0",
    "debug-logger": "^0.4.1",
    "dot-tpl-loader": "^1.2.1",
    "express": "^4.16.3",
    "extract-hoc": "0.0.5",
    "is": "^3.3.0",
    "minimist": "^1.2.0",
    "moment": "^2.23.0",
    "node-watch": "^0.6.0",
    "nodemon": "^1.18.7",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-rnd": "^9.0.4",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "request": "^2.85.0",
    "shortid": "^2.2.8",
    "superagent": "^3.8.2",
    "url-loader": "^1.1.2"
  },
  "devDependencies": {
    "wpi-react-redux-sass-ssr": "^1.1.3",
    "webpack-inherit": "^1.4.9"
  }
}
