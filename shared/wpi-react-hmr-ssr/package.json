{
  "name": "wpi-react-hmr-ssr",
  "version": "0.1.2",
  "description": "An inheritable boilerplate with webpack 4, react, sass, hmr, SSR, etc",
  "license": "MIT",
  "main": "./Server.js",
  "author": "Nathan Braun <n8tz.js@gmail.com>",
  "wpInherit": {
    "default": {
      "rootFolder": "App",
      "config": "./etc/wp/webpack.config.www.js",
      "extend": []
    },
    "api": {
      "rootFolder": "App",
      "config": "./etc/wp/webpack.config.api.js",
      "extend": []
    },
    "Comp": {
      "rootFolder": "Comp",
      "config": "./etc/wp/webpack.config.comp.js",
      "vars": {
        "rootAlias": "Comp",
        "webpackPatch": {}
      },
      "extend": []
    }
  },
  "scripts": {
    "install": "npm rebuild node-sass||0",
    "build": "wpi& wpi :api",
    "dev-api": "wpi :api -w",
    "start-dev": "concurrently \"wpi-dev-server --hot\" \"wpi :api -w\" \"sleep 3&&nodemon --watch ./dist/App.server.js --inspect=[::]:9229 ./Server.js -p 9701\"",
    "start": "node ./Server.js -p 9501",
    "runBrowser": "start http://localhost:9501"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "debounce": "^1.2.0",
    "debug-logger": "^0.4.1",
    "express": "^4.16.3",
    "is": "^3.3.0",
    "minimist": "^1.2.0",
    "moment": "^2.23.0",
    "prop-types": "^15.6.0",
    "shortid": "^2.2.8",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "superagent": "^3.8.2",
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.0",
    "@babel/plugin-proposal-decorators": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.3.1",
    "autoprefixer": "^9.4.1",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.21",
    "clean-webpack-plugin": "^1.0.0",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "dot-tpl-loader": "^1.2.1",
    "file-loader": "^2.0.0",
    "hoist-non-react-methods": "^1.1.0",
    "hoist-non-react-statics": "^3.2.1",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy": "^1.17.0",
    "json-loader": "^0.5.7",
    "lodash": "^4.17.11",
    "mini-css-extract-plugin": "^0.4.5",
    "node-sass": "^4.8.3",
    "nodemon": "^1.18.7",
    "npm-run-all": "^4.1.5",
    "null-loader": "^0.1.1",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss": "^7.0.6",
    "postcss-calc": "^7.0.1",
    "postcss-comment": "^2.0.0",
    "postcss-custom-properties": "^8.0.9",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-mixins": "^6.2.0",
    "postcss-nested": "^4.1.0",
    "postcss-simple-vars": "^5.0.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^4.6.3",
    "sass-loader": "^7.1.0",
    "source-map-support": "^0.5.9",
    "style-loader": "^0.23.1",
    "svg-react-loader": "^0.4.6",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "universal-webpack": "^0.6.6",
    "url-loader": "^1.1.2",
    "webpack": "^4.27.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.0.0",
    "webpack-dev-middleware": "^3.5.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-inherit": "file:../../../webpack-inherit",
    "webpack-visualizer-plugin": "^0.1.11"
  }
}
