{
  "name": "wp4-react-redux-sass-hmr",
  "version": "1.0.0",
  "description": "An inheritable boilerplate with webpack 4, react, redux, sass, hmr, SSR, etc",
  "license": "MIT",
  "main": "./Server.js",
  "author": "Nathan Braun <n8tz.js@gmail.com>",
  "wpInherit": {
    "default": {
      "rootFolder": "App",
      "extend": [
        "wpi-react-hmr-ssr"
      ]
    },
    "api": {
      "rootFolder": "App",
      "extend": [
        "wpi-react-hmr-ssr"
      ]
    }
  },
  "scripts": {
    "build": "wpi& wpi :api",
    "dev-api": "wpi :api -w",
    "start-dev": "concurrently \"wpi-dev-server --hot\" \"wpi :api -w\" \"sleep 3&&nodemon --watch ./dist/App.server.js --inspect=[::]:9229 ./Server.js -p 9701\"",
    "start": "node ./Server.js -p 8080",
    "runBrowser": "start http://localhost:8080"
  },
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^4.6.3",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.7",
    "redux-thunk": "^2.3.0",
    "wpi-react-hmr-ssr": "file:../../shared/wpi-react-hmr-ssr"
  }
}
