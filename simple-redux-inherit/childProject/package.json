{
  "name": "childProject",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "main": "./Server.js",
  "author": "Nathan Braun <caipilabs@gmail.com>",
  "wpInherit": {
    "default": {
      "rootFolder": "App",
      "extend": [
        "parentProject",
        "wp4-react-redux"
      ]
    },
    "api": {
      "rootFolder": "App",
      "extend": [
        "parentProject",
        "wp4-react-redux"
      ]
    }
  },
  "scripts": {
    "build": "wpi&wpi :api",
    "dev": "wpi-dev-server --hot",
    "dev-api": "wpi :api -w",
    "start": "nodemon ./Server.js -p 9001",
    "runDebug": "nodemon --inspect=[::]:9229 ./Server.js -p 9001",
    "runBrowser": "start http://localhost:9001&nodemon ./Server.js -p 9001"
  },
  "dependencies": {
    "nodemon": "^1.18.7"
  },
  "devDependencies": {
    "wp4-react-redux": "file:../wp4-react-redux",
    "parentProject": "file:../parentProject",
    "webpack-inherit": "^1.0.9"
  }
}
